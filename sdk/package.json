{
  "name": "@agent-bounty-hunter/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Agent Bounty Hunter API with EIP-712 auth and x402 payments",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun && tsc --emitDeclarationOnly",
    "dev": "bun run --watch src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "bounty",
    "ai-agents",
    "eip712",
    "x402",
    "crypto"
  ],
  "license": "MIT"
}
